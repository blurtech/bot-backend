{
  "name": "chatbot",
  "version": "0.1.0",
  "private": true,
  "main": "./bin/www",
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "lint": "eslint . --ext .js",
    "lint-fix": "eslint . --ext .js --fix",
    "doc": "jsdoc -c jsdoc.conf.json",
    "test": "mocha ./tests --recursive",
    "pre-push": "npm run lint && npm run test && npm run doc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blurtech/bot-backend.git"
  },
  "bugs": {
    "url": "https://github.com/blurtech/bot-backend/issues"
  },
  "dependencies": {
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "debug": "4.1.1",
    "express": "4.16.4",
    "express-session": "1.15.6",
    "fuzzball": "1.2.0",
    "http-errors": "1.6.3",
    "lodash": "^4.17.11",
    "mongoose": "5.4.19",
    "morgan": "1.9.1",
    "passport-vkontakte": "0.3.2",
    "pug": "2.0.3"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "chai-http": "4.2.1",
    "eslint": "5.15.3",
    "jsdoc": "3.5.5",
    "mocha": "6.0.2",
    "nodemon": "1.18.10"
  }
}
